#!/bin/bash
export LESSOPEN='| ~/devsetup/service-fabrik.lessopen.sh %s'
source $HOME/devsetup/bin/global/helper-functions
source $HOME/devsetup/bin/global/proxy
alias t='attach_to_tmux rivendell rivendell'
# Note to self: this is parameterized in my office dotfiles using env values,
# hence not versioning a .shrc.local in git
attach_to_tmux rivendell rivendell

local_load_nvm() {
  export NVM_DIR="$HOME/.nvm"
  . "/usr/local/opt/nvm/nvm.sh"
}
